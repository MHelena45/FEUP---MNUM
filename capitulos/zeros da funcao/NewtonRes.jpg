#include <iomanip>
#include <iostream>
using namespace std;
/* 
2.1 - No maxima 
	plot2d(-x+40*cos(sqrt(x)) +3,[x,0, 60]);
*/
double g(double x)
{
	return -x + 40 * cos(pow(x, 0.5)) + 3;
}

/* No maxima diff(-x+40*cos(sqrt(x)) +3,x); */
double gl(double x)
{
	return -1 - ((20 * sin(pow(x, 0.5)) / (pow(x, 0.5))));
}
int main()
{
	cout << "xn" << setw(15) << "g(xn)" << endl;
	double xn1, xn = 1.7;
	for (int i = 1; i < 5; i++)
	{
		cout << xn << setw(15) << g(xn) << endl;
		xn = xn - (g(xn) / gl(xn));
	}
	system("pause");
	return 0;
}